{
    "name": "rollup-server",
    "version": "1.0.0",
    "description": "Rollup Server",
    "main": "src/main.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "test": "NODE_ENV=test node_modules/.bin/hardhat test",
        "clean": "NODE_ENV=test node_modules/.bin/hardhat clean",
        "build": "NODE_ENV=test node_modules/.bin/hardhat compile",
        "start:hardhat": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts",
        "start:mainnet": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts --network main_net",
        "start:testnet": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts --network test_net",
        "start": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts --network production_net",
        "lint": "node_modules/.bin/tslint --config tslint.json \"{src,test}/**/*.ts\"",
        "lint-fix": "node_modules/.bin/tslint --fix --config tslint.json \"{src,test}/**/*.ts\"",
        "prettier": "node_modules/.bin/prettier --write --config .prettierrc 'contracts/**/*.sol' 'test/**/*.ts'",
        "debug:Endpoints": "NODE_ENV=test node_modules/.bin/hardhat test test/service/Endpoints.test.ts",
        "debug:Config": "NODE_ENV=test node_modules/.bin/hardhat test test/service/Config.test.ts",
        "debug:BOA": "NODE_ENV=test node_modules/.bin/hardhat test test/service/BOA.test.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bosagora/rollup.git"
    },
    "author": "BOSAGORA Foundation",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bosagora/rollup/issues"
    },
    "homepage": "https://github.com/bosagora/rollup#readme",
    "dependencies": {
        "@ethersproject/experimental": "^5.6.3",
        "@nomiclabs/hardhat-ethers": "^2.0.4",
        "@nomiclabs/hardhat-etherscan": "^2.1.8",
        "@nomiclabs/hardhat-waffle": "^2.0.2",
        "@nomiclabs/hardhat-web3": "^2.0.0",
        "@openzeppelin/contracts": "^4.5.0",
        "@typechain/ethers-v5": "^7.2.0",
        "@typechain/hardhat": "^2.3.1",
        "@types/argparse": "^2.0.2",
        "@types/chai": "^4.3.0",
        "@types/cors": "^2.8.8",
        "@types/express": "^4.17.8",
        "@types/extend": "^3.0.1",
        "@types/ip": "^1.1.0",
        "@types/js-yaml": "^4.0.5",
        "@types/mocha": "^9.1.0",
        "@types/node": "^12.20.43",
        "@types/node-cron": "^3.0.1",
        "@types/urijs": "^1.19.12",
        "ajv": "^6.12.6",
        "argparse": "^2.0.1",
        "assert": "^2.0.0",
        "axios": "^0.26.0",
        "body-parser": "^1.19.0",
        "bs58": "^5.0.0",
        "chai": "^4.3.6",
        "coingecko-api-v3": "^0.0.13",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "ethereum-waffle": "^3.4.0",
        "ethers": "^5.5.4",
        "express": "^4.17.1",
        "express-validator": "^6.14.0",
        "extend": "^3.0.2",
        "hardhat": "^2.8.3",
        "hardhat-change-network": "^0.0.7",
        "hardhat-gas-reporter": "^1.0.7",
        "ip": "^1.1.5",
        "ipfs-mini": "^1.1.5",
        "mocha": "^8.0.1",
        "moment": "^2.29.1",
        "mybatis-mapper": "^0.6.7",
        "mysql2": "^2.3.3",
        "node-cron": "^3.0.0",
        "prettier": "^2.5.1",
        "prettier-plugin-solidity": "^1.0.0-beta.13",
        "prompt": "^1.3.0",
        "smart-buffer": "^4.1.0",
        "solhint": "^3.3.6",
        "solidity-coverage": "^0.8.2",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.5.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typechain": "^5.2.0",
        "typescript": "^4.5.5",
        "urijs": "^1.19.7",
        "web3": "^1.7.3",
        "winston": "^3.3.3",
        "yaml-env-defaults": "^1.1.3"
    }
}
