{
    "name": "rollup-server",
    "version": "1.0.0",
    "description": "Rollup Server",
    "main": "src/main.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "test": "NODE_ENV=test node_modules/.bin/hardhat test",
        "clean": "NODE_ENV=test node_modules/.bin/hardhat clean",
        "build": "NODE_ENV=test node_modules/.bin/hardhat compile",
        "start:hardhat": "NODE_ENV=development node_modules/.bin/hardhat run src/main.ts",
        "start:mainnet": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts --network main_net",
        "start:testnet": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts --network test_net",
        "start": "NODE_ENV=production node_modules/.bin/hardhat run src/main.ts --network production_net",
        "lint": "node_modules/.bin/tslint --config tslint.json \"{src,test}/**/*.ts\"",
        "lint-fix": "node_modules/.bin/tslint --fix --config tslint.json \"{src,test}/**/*.ts\"",
        "prettier": "node_modules/.bin/prettier --write --config .prettierrc 'contracts/**/*.sol' 'test/**/*.ts'",
        "debug:Config": "NODE_ENV=test node_modules/.bin/hardhat test test/service/Config.test.ts",
        "debug:BOA": "NODE_ENV=test node_modules/.bin/hardhat test test/service/BOA.test.ts",
        "debug:Storage": "NODE_ENV=test node_modules/.bin/hardhat test test/service/Storage.test.ts",
        "debug:TransactionPool": "NODE_ENV=test node_modules/.bin/hardhat test test/blockchain/TransactionPool.test.ts",
        "debug:Node": "NODE_ENV=test node_modules/.bin/hardhat test test/blockchain/Node.test.ts",
        "debug:SendBlock": "NODE_ENV=test node_modules/.bin/hardhat test test/blockchain/SendBlock.test.ts",
        "debug:Router": "NODE_ENV=test node_modules/.bin/hardhat test test/service/RollupRouter.test.ts",
        "debug:Server": "NODE_ENV=test node_modules/.bin/hardhat test test/service/RollupServer.test.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bosagora/rollup-server.git"
    },
    "author": "BOSAGORA Foundation",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bosagora/rollup-server/issues"
    },
    "homepage": "https://github.com/bosagora/rollup-server#readme",
    "dependencies": {
        "@ethersproject/experimental": "^5.6.3",
        "@nomiclabs/hardhat-ethers": "^2.1.0",
        "@nomiclabs/hardhat-etherscan": "^3.1.0",
        "@nomiclabs/hardhat-waffle": "^2.0.2",
        "@nomiclabs/hardhat-web3": "^2.0.0",
        "@openzeppelin/contracts": "^4.8.0",
        "@typechain/ethers-v5": "^10.1.0",
        "@typechain/hardhat": "^6.1.2",
        "@types/argparse": "^2.0.2",
        "@types/chai": "^4.3.0",
        "@types/cors": "^2.8.8",
        "@types/express": "^4.17.8",
        "@types/extend": "^3.0.1",
        "@types/ip": "^1.1.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "^12.20.43",
        "@types/node-cron": "^3.0.1",
        "@types/sqlite3": "^3.1.8",
        "@types/urijs": "^1.19.12",
        "argparse": "^2.0.1",
        "assert": "^2.0.0",
        "axios": "^0.27.2",
        "body-parser": "^1.19.0",
        "bs58": "^5.0.0",
        "chai": "^4.3.6",
        "chai-http": "^4.3.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "ethereum-waffle": "^3.4.0",
        "ethers": "^5.6.9",
        "express": "^4.17.1",
        "express-validator": "^6.14.2",
        "extend": "^3.0.2",
        "hardhat": "^2.10.0",
        "hardhat-gas-reporter": "^1.0.7",
        "ip": "^1.1.5",
        "ipfs-mini": "^1.1.5",
        "mkdirp": "^1.0.4",
        "mocha": "^8.0.1",
        "node-cron": "^3.0.0",
        "prettier": "^2.5.1",
        "prettier-plugin-solidity": "^1.0.0",
        "rollup-pm-sdk": "^1.0.5",
        "smart-buffer": "^4.1.0",
        "solidity-coverage": "^0.8.2",
        "sqlite3": "^5.1.2",
        "ts-node": "^10.5.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typechain": "^8.1.0",
        "typescript": "^4.5.5",
        "urijs": "^1.19.7",
        "web3": "^1.7.4",
        "winston": "^3.3.3",
        "yaml-env-defaults": "^1.1.3"
    }
}
